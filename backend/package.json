{
    "name": "teravin-backend",
    "version": "1.0.0",
    "description": "Teravin Backend",
    "main": "index.js",
    "scripts": {
        "serve": "nodemon app.js",
        "start": "pm2 start app.js --name teravin-backend --watch",
        "db:init": "node_modules/.bin/sequelize db:create",
        "db:migrate": "node_modules/.bin/sequelize db:migrate",
        "db:reset": "node_modules/.bin/sequelize db:migrate:undo:all",
        "db:drop": "node_modules/.bin/sequelize db:drop"
    },
    "keywords": [
        "express",
        "docker"
    ],
    "author": "l4zydev",
    "license": "ISC",
    "dependencies": {
        "base64-img": "^1.0.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.10.0",
        "fs-extra": "^9.0.1",
        "is-base64": "^1.1.0",
        "multer": "^1.4.2",
        "mysql2": "^2.1.0",
        "nodemon": "^2.0.4",
        "sequelize": "^5.21.13",
        "sequelize-cli": "^6.2.0"
    }
}
